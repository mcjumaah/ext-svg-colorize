{
	"name": "@mcjumaah/ext-svg-colorize",
	"description": "Vue component to easily change external SVGs' color",
	"version": "1.0.4",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./styles.css": "./dist/style.css"
	},
	"browser": {
		"./styles.css": "./dist/style.css"
	},
	"types": "./dist/ViewerPlugin.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
	},
	"dependencies": {
		"vue": "^3.3.11"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.3.3",
		"@tsconfig/node18": "^18.2.2",
		"@types/node": "^18.19.3",
		"@vitejs/plugin-vue": "^4.5.2",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.5.0",
		"eslint": "^8.49.0",
		"eslint-plugin-vue": "^9.17.0",
		"npm-run-all2": "^6.1.1",
		"rollup-plugin-typescript2": "^0.31.2",
		"typescript": "~5.3.0",
		"vite": "^5.0.10",
		"vue-tsc": "^1.8.25"
	},
	"keywords": [
		"svg",
		"vue",
		"nuxt",
		"color"
	],
	"author": "Mark Jumar Perez",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mcjumaah/ext-svg-colorize/issues"
	},
	"homepage": "https://github.com/mcjumaah/ext-svg-colorize#readme"
}
